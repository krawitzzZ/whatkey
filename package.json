{
  "name": "whatkey",
  "displayName": "WhatKey",
  "description": "A customizable command menu with key sequences and search functionality",
  "version": "0.0.5",
  "type": "module",
  "publisher": "krawitzzZ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/krawitzzZ/whatkey"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Keymaps",
    "Other"
  ],
  "keywords": [
    "keybindings",
    "commands",
    "menu",
    "whichkey",
    "whatkey",
    "productivity"
  ],
  "activationEvents": [],
  "main": "./dist/extension.cjs",
  "contributes": {
    "commands": [
      {
        "command": "whatkey.show",
        "title": "Show Command Menu",
        "category": "WhatKey"
      },
      {
        "command": "whatkey.search",
        "title": "Search Commands",
        "category": "WhatKey"
      }
    ],
    "configuration": {
      "title": "WhatKey",
      "properties": {
        "whatkey.bindings": {
          "type": "array",
          "default": [],
          "description": "Command bindings for the WhatKey menu",
          "items": {
            "type": "object",
            "properties": {
              "key": {
                "type": "string",
                "minLength": 1,
                "maxLength": 2,
                "description": "The key (1-2 characters) to trigger this binding"
              },
              "name": {
                "type": "string",
                "description": "Display name for this binding"
              },
              "type": {
                "type": "string",
                "enum": [
                  "command",
                  "commands",
                  "shell",
                  "submenu"
                ],
                "description": "Type of binding: command (single VS Code command), commands (multiple VS Code commands), shell (execute shell command), or submenu (nested menu)"
              },
              "icon": {
                "type": "string",
                "description": "VS Code icon identifier (e.g., 'file', 'code'). See https://code.visualstudio.com/api/references/icons-in-labels"
              },
              "detail": {
                "type": "string",
                "description": "Additional detail shown in the menu when showDetail is enabled"
              },
              "command": {
                "type": "string",
                "description": "VS Code command to execute (for type: command), or shell command/executable to run (for type: shell)"
              },
              "commands": {
                "type": "array",
                "items": {
                  "oneOf": [
                    {
                      "type": "string"
                    },
                    {
                      "type": "object",
                      "properties": {
                        "command": {
                          "type": "string"
                        },
                        "args": {}
                      },
                      "required": [
                        "command"
                      ]
                    }
                  ]
                },
                "description": "Array of VS Code commands to execute in sequence (for type: commands)"
              },
              "args": {
                "description": "Arguments to pass to the command (for type: command or shell)"
              },
              "cwd": {
                "type": "string",
                "description": "Working directory for shell commands (for type: shell). Defaults to workspace root."
              },
              "output": {
                "type": "string",
                "enum": [
                  "silent",
                  "channel",
                  "notification",
                  "terminal"
                ],
                "default": "silent",
                "description": "How to handle shell command output (for type: shell): silent (no output), channel (Output panel), notification (message popup), terminal (visible terminal)"
              },
              "items": {
                "type": "array",
                "description": "Nested bindings (for type: submenu)",
                "items": {
                  "type": "object",
                  "properties": {
                    "key": {
                      "type": "string",
                      "minLength": 1,
                      "maxLength": 2,
                      "description": "The key (1-2 characters) to trigger this binding"
                    },
                    "name": {
                      "type": "string",
                      "description": "Display name for this binding"
                    },
                    "type": {
                      "type": "string",
                      "enum": [
                        "command",
                        "commands",
                        "shell",
                        "submenu"
                      ],
                      "description": "Type of binding"
                    },
                    "icon": {
                      "type": "string",
                      "description": "VS Code icon identifier"
                    },
                    "detail": {
                      "type": "string",
                      "description": "Additional detail shown in the menu"
                    },
                    "command": {
                      "type": "string",
                      "description": "Command to execute (for type: command or shell)"
                    },
                    "commands": {
                      "type": "array",
                      "description": "Commands to execute in sequence (for type: commands)"
                    },
                    "args": {
                      "description": "Arguments to pass to the command"
                    },
                    "cwd": {
                      "type": "string",
                      "description": "Working directory (for type: shell)"
                    },
                    "output": {
                      "type": "string",
                      "enum": [
                        "silent",
                        "channel",
                        "notification",
                        "terminal"
                      ],
                      "default": "silent",
                      "description": "Output handling (for type: shell)"
                    },
                    "items": {
                      "type": "array",
                      "description": "Nested bindings (for type: submenu)"
                    }
                  },
                  "required": [
                    "key",
                    "name",
                    "type"
                  ]
                }
              }
            },
            "required": [
              "key",
              "name",
              "type"
            ]
          }
        },
        "whatkey.sortOrder": {
          "type": "string",
          "enum": [
            "alphabetical",
            "custom"
          ],
          "enumDescriptions": [
            "Sort menu items alphabetically by key",
            "Keep menu items in the order defined in configuration"
          ],
          "default": "custom",
          "description": "Sort order for menu items (items of type 'bindings' are always at the top)"
        },
        "whatkey.showIcons": {
          "type": "boolean",
          "default": true,
          "description": "Show icons in the command menu"
        },
        "whatkey.showDetail": {
          "type": "boolean",
          "default": true,
          "description": "Whether to show detail in the command menu (e.g., command IDs, number of commands)"
        },
        "whatkey.keySequenceTimeout": {
          "type": "number",
          "default": 350,
          "description": "Timeout in milliseconds for multi-character key sequences. When typing keys quickly within this window, they are combined to match multi-char bindings (e.g., 'qa' to close all editors). Set to 0 to disable."
        },
        "whatkey.bindingsMergeStrategy": {
          "type": "string",
          "enum": [
            "merge",
            "replace"
          ],
          "enumDescriptions": [
            "Merge user bindings with default bindings. User items are appended to matching submenus, new keys are added.",
            "Replace default bindings entirely with user-defined bindings."
          ],
          "default": "merge",
          "description": "Strategy for combining default bindings with user-defined bindings"
        }
      }
    },
    "keybindings": [
      {
        "command": "whatkey.show",
        "key": "ctrl+alt+;",
        "mac": "cmd+alt+;",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "dev": "rslib build --watch",
    "build": "rslib build",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check": "pnpm typecheck && pnpm format:check && pnpm lint && pnpm build && pnpm package",
    "vscode:prepublish": "pnpm run build && pnpm run test:coverage",
    "package": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies",
    "generate:schema": "tsx scripts/generate-schema.ts"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@golevelup/ts-jest": "^1.1.1",
    "@rslib/core": "0.18.5",
    "@swc/core": "1.15.7",
    "@swc/jest": "0.2.39",
    "@types/jest": "30.0.0",
    "@types/node": "22.19.3",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "8.50.0",
    "@typescript-eslint/parser": "8.50.0",
    "@vscode/vsce": "3.7.1",
    "eslint": "9.39.2",
    "jest": "30.2.0",
    "prettier": "3.7.4",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.0",
    "zod": "4.2.1"
  },
  "packageManager": "pnpm@10.17.1"
}
